{
 "name": "kiddo-kingdom",
 "version": "0.1.0",
 "private": true,
 "scripts": {
  "dev": "next dev",
  "build": "next build",
  "start": "next start",
  "lint": "next lint",
  "format": "prettier --config .prettierrc.js \"**/*.{ts,tsx}\" --write",
  "prettier": "prettier --config .prettierrc.js \"**/*.{ts,tsx}\" --check",
  "test": "jest",
  "test:coverage": "jest --ci --runInBand --coverage --reporters='default' --reporters='jest-junit' --watchAll=false",
  "prepare": "husky install",
  "pre-commit": "npm run prettier && npm run lint && npm test",
  "pre-push": "npx validate-branch-name"
 },
 "validate-branch-name": {
  "pattern": "^(feature|release|fix|hotfix|docs|build|test|refactor)/([A-Z-]+-\\d+/)?[a-z-]+",
  "errorMsg": "Check your branch name. Use 'git branch -m new-branch-name' to rename it."
 },
 "dependencies": {
  "@ant-design/cssinjs": "^1.16.2",
  "@ant-design/icons": "^5.2.5",
  "@ant-design/static-style-extract": "^1.0.1",
  "@commercetools/platform-sdk": "^4.11.0",
  "@commercetools/sdk-client-v2": "^2.2.0",
  "@types/country-list": "^2.1.1",
  "@types/node": "20.5.1",
  "@types/react": "18.2.20",
  "@types/react-dom": "18.2.7",
  "@typescript-eslint/parser": "^6.3.0",
  "antd": "^5.8.3",
  "country-list": "^2.3.0",
  "eslint": "8.47.0",
  "eslint-config-next": "13.4.18",
  "moment": "^2.29.4",
  "next": "13.4.18",
  "postal-codes-js": "^2.5.2",
  "react": "18.2.0",
  "react-dom": "18.2.0",
  "typescript": "5.1.6"
 },
 "devDependencies": {
  "@testing-library/jest-dom": "^5.17.0",
  "@testing-library/react": "^14.0.0",
  "@testing-library/user-event": "^14.4.3",
  "@types/jest": "^29.5.3",
  "@typescript-eslint/eslint-plugin": "^6.2.1",
  "cross-env": "^7.0.3",
  "eslint-config-airbnb-base": "^15.0.0",
  "eslint-config-airbnb-typescript": "^17.1.0",
  "eslint-config-prettier": "^8.10.0",
  "eslint-plugin-jest": "^27.2.3",
  "eslint-plugin-jest-dom": "^5.0.2",
  "eslint-plugin-prettier": "^5.0.0",
  "eslint-plugin-testing-library": "^5.11.0",
  "husky": "^8.0.0",
  "jest": "^29.6.2",
  "jest-environment-jsdom": "^29.6.2",
  "jest-junit": "^16.0.0",
  "prettier": "^3.0.1",
  "ts-jest": "^29.1.1",
  "ts-node": "^10.9.1",
  "tslib": "^2.6.1"
 }
}
